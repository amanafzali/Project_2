    "\n",
    "btc_df['US_Holiday'] = [1 if day in us_holidays else 0 for day in btc_df['Date']]\n",
    "\n",
    "# create US_Market_Open flag from Hour, Weekday, US_Holiday columns\n",
    "#  pythonic Monday = 0, assume institutional capital active M-F 9am-5pm\n",
